{
    "cli": {
      "appVersionSource": "local"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal"
      },
      "ios-simulator": {
        "extends": "development",
        "ios": {
          "simulator": true
        }
      },
      "preview": {
        "distribution": "internal"
      },
      "preview-simulator": {
        "extends": "preview",
        "ios": {
          "simulator": true
        }
      },
      "production": {
        "autoIncrement": true,
        "ios": {
          "image": "macos-sequoia-15.6-xcode-26.0"
        }
      }
    },
    "submit": {
      "production": {}
    }
  }
  